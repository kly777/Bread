{
        "name": "bread",
        "description": "a extension for reading",
        "private": true,
        "version": "0.1.27",
        "type": "module",
        "scripts": {
                "dev": "wxt",
                "dev:firefox": "wxt -b firefox",
                "build": "wxt build",
                "build:firefox": "wxt build -b firefox",
                "zip": "wxt zip",
                "zip:firefox": "wxt zip -b firefox",
                "submit": "wxt submit",
                "compile": "vue-tsc --noEmit",
                "postinstall": "wxt prepare",
                "fmt": "prettier --write .",
                "lint": "eslint .",
                "lint:fix": "eslint . --fix"
        },
        "dependencies": {
                "@wxt-dev/storage": "^1.2.6",
                "terser": "^5.44.1",
                "tinycolor2": "^1.6.0",
                "vue": "^3.5.24"
        },
        "devDependencies": {
                "@eslint/js": "^9.39.1",
                "@types/chrome": "^0.1.29",
                "@types/tinycolor2": "^1.4.6",
                "@typescript-eslint/eslint-plugin": "^8.46.4",
                "@typescript-eslint/parser": "^8.46.4",
                "@wxt-dev/module-vue": "^1.0.3",
                "eslint": "^9.39.1",
                "eslint-plugin-vue": "^10.5.1",
                "typescript": "5.9.3",
                "vue-eslint-parser": "^10.2.0",
                "vue-tsc": "^3.1.3",
                "wxt": "^0.20.11"
        }
}