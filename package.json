{
        "name": "bread",
        "description": "a extension for reading",
        "private": true,
        "version": "0.1.29",
        "type": "module",
        "scripts": {
                "dev": "rollup -c --environment NODE_ENV:development",
                "dev:firefox": "rollup -c --environment NODE_ENV:development,BROWSER:firefox && web-ext run --source-dir=dist/firefox --firefox=firefoxdeveloperedition",
                "dev:firefox:only": "web-ext run --source-dir=dist/firefox --firefox=firefox",
                "dev:chrome": "rollup -c --environment NODE_ENV:development,BROWSER:chrome",
                "build": "rollup -c --environment NODE_ENV:production",
                "build:firefox": "rollup -c --environment NODE_ENV:production,BROWSER:firefox",
                "build:chrome": "rollup -c --environment NODE_ENV:production,BROWSER:chrome",
                "zip": "node scripts/zip.js",
                "zip:firefox": "BROWSER=firefox node scripts/zip.js",
                "zip:chrome": "BROWSER=chrome node scripts/zip.js",
                "compile": "tsc --noEmit",
                "fmt": "prettier --write .",
                "lint": "eslint .",
                "lint:fix": "eslint . --fix",
                "clean": "rm -rf dist dist-zip"
        },
        "dependencies": {},
        "devDependencies": {
                "@eslint/js": "^9.39.2",
                "@rollup/plugin-alias": "^6.0.0",
                "@rollup/plugin-commonjs": "^29.0.0",
                "@rollup/plugin-node-resolve": "^16.0.3",
                "@rollup/plugin-replace": "^6.0.3",
                "@rollup/plugin-terser": "^0.4.4",
                "@rollup/plugin-typescript": "^12.3.0",
                "@types/chrome": "^0.1.32",
                "@typescript-eslint/eslint-plugin": "^8.49.0",
                "@typescript-eslint/parser": "^8.49.0",
                "eslint": "^9.39.2",
                "postcss": "^8.5.6",
                "postcss-csso": "^6.0.1",
                "postcss-import": "^16.1.1",
                "rollup": "^4.53.3",
                "rollup-plugin-copy": "^3.5.0",
                "typescript": "5.9.3",
                "web-ext": "^9.2.0"
        }
}
